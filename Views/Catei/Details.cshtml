@model Proiect_Master_Andrei_Preotu.Models.Catel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Catel</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nume)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nume)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rasa)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rasa)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Stapan)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Stapan.ID)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Premii)
        </dt>
        <dd class="col-sm-10">
            <table class="table">
                <tr>
                    <th>Nume Competitie</th>
                    <th>Loc</th>
                </tr>
                @foreach (var item in Model.Premii)
                {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Nume_Competitie)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Loc)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Suma)
                </td>
            </tr>
                }
            </table>
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ID">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
